<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BancoController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Cliente">
             <content>
                <fx:include fx:id="tabCliente" source="ClienteCrud.fxml" />
             </content>
          </Tab>
          <Tab text="Empleado">
             <content>
                <fx:include fx:id="tabEmpleado" source="EmpleadoCrud.fxml" />
             </content>
          </Tab>
        </tabs>
      </TabPane>

   </children>
</AnchorPane>
